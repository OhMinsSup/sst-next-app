{
  "name": "sst-nextjs-template",
  "version": "0.0.1-beta.0",
  "description": "nextjs sst template project",
  "scripts": {
    "dev": "run-s dev:*",
    "dev:cli": "node dist/cli.js --env local",
    "dev:start": "next",
    "start": "next start",
    "build": "next build",
    "export": "next export",
    "fix": "run-p fix:*",
    "fix:types": "tsc --noEmit",
    "fix:eslint-fix": "next lint --fix",
    "fix:prettier-fix": "prettier --write '**/*.{js,jsx,md,ts,tsx}'",
    "deploy:dev": "run-s sst:dev:*",
    "sst:dev:partytown": "partytown copylib public/~partytown",
    "sst:dev:cli": "node dist/cli.js --env dev",
    "sst:dev:deploy": "sst deploy",
    "remove:staging": "run-s sst:remove:*",
    "sst:remove:cli": "node dist/cli.js --env dev",
    "sst:remove:rm": "sst remove",
    "cli:start": "tsup --entry.cli scripts/cli/index.ts --minify",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:ci": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "db:init": "prisma generate",
    "db:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "nextjs",
    "sst",
    "template"
  ],
  "author": "",
  "license": "ISC",
