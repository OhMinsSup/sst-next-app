{
  "name": "sst-nextjs-template",
  "version": "0.0.1-beta.0",
  "description": "nextjs sst template project",
  "scripts": {
    "dev": "run-s dev:*",
    "dev:cli": "node dist/cli.js --env local",
    "dev:start": "next",
    "start": "next start",
    "build": "next build",
    "export": "next export",
    "fix": "run-p fix:*",
    "fix:types": "tsc --noEmit",
    "fix:eslint-fix": "next lint --fix",
    "fix:prettier-fix": "prettier --write '**/*.{js,jsx,md,ts,tsx}'",
    "deploy:dev": "run-s sst:dev:*",
    "sst:dev:partytown": "partytown copylib public/~partytown",
    "sst:dev:cli": "node dist/cli.js --env dev",
    "sst:dev:deploy": "sst deploy",
    "remove:staging": "run-s sst:remove:*",
    "sst:remove:cli": "node dist/cli.js --env dev",
    "sst:remove:rm": "sst remove",
    "cli:start": "tsup --entry.cli scripts/cli/index.ts --minify",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:ci": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "db:init": "prisma generate",
    "db:dev": "prisma migrate dev",
    "db:push": "prisma db push"
  },
  "keywords": [
    "nextjs",
    "sst",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "extends @anansi/browserslist-config"
  ],
  "dependencies": {
    "@builder.io/partytown": "^0.8.1",
    "@dicebear/collection": "^7.0.1",
    "@dicebear/core": "^7.0.1",
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.5.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@sentry/nextjs": "^7.75.1",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tiptap/extension-character-count": "^2.1.12",
    "@tiptap/extension-placeholder": "^2.1.12",
    "@tiptap/pm": "^2.1.12",
    "@tiptap/react": "^2.1.12",
    "@tiptap/starter-kit": "^2.1.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "highlight.js": "^11.9.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.289.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-virtuoso": "^4.6.2",
    "server-only": "^0.0.1",
    "superjson": "^2.2.0",
    "tailwind-merge": "^1.14.0",
    "unique-username-generator": "^1.2.0",
    "use-debounce": "^9.0.4",
    "zod": "^3.22.4",
    "zustand": "^4.4.4"
  },
  "devDependencies": {
    "@anansi/browserslist-config": "^1.4.3",
    "@playwright/test": "^1.39.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/react": "^14.0.0",
    "@types/cookie": "^0.5.3",
    "@types/lodash-es": "^4.17.10",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "aws-cdk-lib": "2.103.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "constructs": "10.3.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.5.2",
    "sst": "^2.32.2",
    "tailwindcss": "^3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
