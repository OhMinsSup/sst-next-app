{
  "name": "sst-nextjs-template",
  "version": "0.0.1-beta.0",
  "description": "nextjs sst template project",
  "scripts": {
    "local": "run-s local:*",
    "local:cli": "node dist/cli.js --env local",
    "local:start": "sst bind next dev",
    "dev": "run-s dev:*",
    "dev:cli": "node dist/cli.js --env local",
    "dev:start": "next",
    "start": "next start",
    "build": "next build",
    "export": "next export",
    "fix": "run-p fix:*",
    "fix:types": "tsc --noEmit",
    "fix:eslint-fix": "next lint --fix",
    "fix:prettier-fix": "prettier --write '**/*.{js,jsx,md,ts,tsx}'",
    "deploy:dev": "run-s sst:dev:*",
    "sst:dev:cli": "node dist/index.js --env dev",
    "sst:dev:deploy": "sst deploy",
    "partytown": "partytown copylib public/~partytown",
    "cli:start": "tsup --entry.cli scripts/cli/index.ts --minify",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:ci": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test"
  },
  "keywords": [
    "nextjs",
    "sst",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "extends @anansi/browserslist-config"
  ],
  "dependencies": {
    "@builder.io/partytown": "^0.7.6",
    "@sentry/nextjs": "^7.47.0",
    "@tanstack/react-query": "^4.28.0",
    "@tanstack/react-query-devtools": "^4.29.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "ky": "^0.33.3",
    "ky-universal": "^0.11.0",
    "lodash-es": "^4.17.21",
    "next": "^13.3.0",
    "next-seo": "^6.0.0",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "sass": "^1.61.0",
    "sitemap": "^7.1.1",
    "web-streams-polyfill": "^3.2.1",
    "zod": "^3.21.4",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@anansi/browserslist-config": "^1.4.2",
    "@playwright/test": "^1.32.2",
    "@testing-library/react": "^14.0.0",
    "@type-challenges/utils": "^0.1.1",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.11",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "chalk": "^5",
    "commander": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.1",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "sst": "^2.5.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  }
}
